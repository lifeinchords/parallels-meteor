{
  "name": "parallels",
  "description": "An app for capturing and playing with data.",
  "version": "0.0.1",
  "private": true,
  "main": "main.js",
  "devDependencies": {
    "adm-zip": "0.4.7",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-3": "^6.16.0",
    "babel-register": "^6.16.3",
    "bower": "1.7.9",
    "chai": "3.5.0",
    "connect-busboy": "0.0.2",
    "css-loader": "^0.25.0",
    "electron": "^1.4.2",
    "electron-builder": "^7.9.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "grunt": "1.0.1",
    "grunt-cli": "1.2.0",
    "grunt-concurrent": "2.3.0",
    "grunt-contrib-clean": "1.0.0",
    "grunt-contrib-connect": "1.0.2",
    "grunt-contrib-copy": "1.0.0",
    "grunt-contrib-jade": "1.0.0",
    "grunt-contrib-jasmine": "1.0.3",
    "grunt-contrib-jshint": "1.0.0",
    "grunt-contrib-watch": "1.0.0",
    "grunt-crx": "0.3.4",
    "grunt-env": "0.4.4",
    "grunt-preprocess": "5.1.0",
    "grunt-sass": "1.2.0",
    "grunt-template-jasmine-requirejs": "0.2.3",
    "gulp-sass": "^2.3.2",
    "jit-grunt": "0.10.0",
    "js-file-loader": "1.0.0",
    "jshint-stylish": "2.2.0",
    "json-loader": "^0.5.4",
    "meteor-stubs": "0.0.6",
    "mocha": "2.5.3",
    "node-sass": "^3.10.1",
    "sass-loader": "^4.0.2",
    "shelljs": "0.7.0",
    "sourced": "0.1.1",
    "sourced-repo-mongo": "0.3.9",
    "spacejam": "1.6.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-merge": "^0.14.1",
    "webpack-validator": "^2.2.8"
  },
  "scripts": {
    "start": "grunt server",
    "desktop": "grunt concurrent:desktop",
    "postinstall": "install-app-deps",
    "pack": "build --dir",
    "dist": "build",
    "start-dev-app": "npm install && NODE_ENV=development electron -r babel-register -r babel-polyfill ./main.development",
    "start-app": "NODE_ENV=production electron ./",
    "build": "npm run build-main && npm run build-renderer",
    "build-main": "NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "local": "grunt build:local",
    "prod": "grunt build",
    "server": "grunt server",
    "dropneo": "grunt env:dev neo4jDrop",
    "resetdb": "neo4j restart && grunt env:dev neo4jReset meteorReset",
    "exportlog": "grunt env:dev exportlog",
    "importlog": "grunt env:dev importlog replayEvents",
    "test": "grunt env:dev jshint connect:clipperTestServer jasmine:clipperUnitTests && ./test-scripts/bin/run_meteor_package_units.sh",
    "test:integration": "grunt env:dev meteorPackageIntegration",
    "test:server": "grunt env:dev meteorPackagesTestServer",
    "clipper": "grunt env:dev connect:clipperServer watch"
  },
  "build": {
    "appId": "org.makeparallels",
    "category": "public.app-category.productivity",
    "asar": true,
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "extraFiles": ["chrome-native-messaging.js", "my_host.js"]
  },
  "directories": {
    "app": "desktop-app"
  },
  "dependencies": {
    "chroma": "0.0.1",
    "meteor-node-stubs": "^0.2.3"
  }
}
